            require-prior-idle-ms = <150>;
            flavor = "balanced";
            hold-trigger-key-positions = <23 0 1 2 3 4 10 11 12 13 14 15 22 24 25 26 27 34 35 36 37 38 39>;
            hold-trigger-on-release;
        };

        stickMOD: stickMOD {
            compatible = "zmk,behavior-sticky-key";
            label = "STICKMOD";
            bindings = <&kp>;
            #binding-cells = <1>;
            release-after-ms = <1500>;
            quick-release;
            ignore-modifiers;
        };

        caps_word: caps_word {
            compatible = "zmk,behavior-caps-word";
            label = "CAPS_WORD";
            #binding-cells = <0>;
            continue-list = <A B C D E F G H I J K L M N O P Q S T U V W X Y Z UNDERSCORE MINUS>;
        };
