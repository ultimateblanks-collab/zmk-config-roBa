    keymap {
        compatible = "zmk,keymap";

        default_layer {
            bindings = <
&kp Q                  &kp W                  &kp E               &kp R                    &kp T                                                  &kp Y                 &kp U                     &kp I               &kp O                   &kp P
&L_MOD_TAP LEFT_WIN A  &L_MOD_TAP LEFT_ALT S  &L_MOD_TAP LCTRL D  &L_MOD_TAP LEFT_SHIFT F  &kp G                 &PipeLine      &kp SINGLE_QUOTE  &kp H                 &R_MOD_TAP RIGHT_SHIFT J  &R_MOD_TAP RCTRL K  &R_MOD_TAP RIGHT_ALT L  &R_MOD_TAP RWIN MINUS
&kp Z                  &kp X                  &kp C               &kp V                    &kp B                 &kp SLASH      &kp SEMICOLON     &kp N                 &kp M                     &lt 5 COMMA         &kp PERIOD              &lt 5 ENTER
&kp HOME               &kp PRINTSCREEN        &kp K_APP           &LThumbLayer 3 PG_UP     &LThumbLayer 5 PG_DN  &kp LCTRL      &kp LEFT_ALT      &RThumbLayer 2 SPACE                                                                        &kp END
            >;

            sensor-bindings = <&inc_dec_kp UP_ARROW DOWN_ARROW>;
        };

        FUNCTION {
            bindings = <
&trans  &trans  &trans  &trans  &trans                       &kp F1  &kp F2  &kp F3  &kp F4  &kp F5
&trans  &trans  &trans  &trans  &trans  &trans      &kp F13  &kp F6  &kp F7  &kp F8  &kp F9  &kp F10
&trans  &trans  &trans  &trans  &trans  &trans      &trans   &trans  &trans  &trans  &trans  &kp F11
&trans  &trans  &trans  &trans  &trans  &trans      &trans   &trans                          &kp F12
            >;
        };

        NUM {
            bindings = <
&none   &shifttab  &kp ENTER  &kp TAB  &none                                   &none       &kp LEFT_SHIFT  &func_word 2  &none   &none
&kp N1  &kp N2     &kp N3     &kp N4   &kp N5   &kp KP_PLUS      &kp KP_MINUS  &kp N6      &kp N7          &kp N8        &kp N9  &kp N0
&kp F1  &kp F2     &kp F3     &kp F4   &kp F5   &kp SLASH        &kp ASTERISK  &kp F6      &kp F7          &kp F8        &kp F9  &kp F10
&trans  &trans     &trans     &kp F11  &kp F12  &trans           &trans        &kp PERIOD                                        &kp PERIOD
            >;
        };

        ARROW {
            bindings = <
&none   &none         &none      &none           &trans                      &trans   &CtPgUp         &kp UP          &CtPgDn          &trans
&none   &kp LEFT_ALT  &kp LCTRL  &kp LEFT_SHIFT  &none   &trans      &trans  &CtHome  &kp LEFT_ARROW  &kp DOWN_ARROW  &kp RIGHT_ARROW  &CtEnd
&none   &none         &none      &none           &trans  &trans      &trans  &trans   &kp HOME        &nav_word 3     &kp END          &trans
&trans  &trans        &trans     &trans          &trans  &trans      &email  &Calte                                                    &trans
            >;

            sensor-bindings = <&inc_dec_kp LC(PAGE_UP) LC(PAGE_DOWN)>;
        };

        MOUSE {
            bindings = <
&trans  &trans  &trans  &trans  &trans                      &trans  &trans    &trans    &trans    &trans
&trans  &trans  &trans  &trans  &trans  &trans      &trans  &trans  &mkp MB1  &mkp MB3  &mkp MB2  &trans
&trans  &trans  &trans  &trans  &trans  &trans      &trans  &trans  &trans    &trans    &trans    &trans
&trans  &trans  &trans  &trans  &trans  &trans      &trans  &trans                                &trans
            >;
        };

        SCROLL {
            bindings = <
&trans  &trans  &trans  &trans  &trans                      &trans  &trans    &trans    &trans    &trans
&trans  &trans  &trans  &trans  &trans  &trans      &trans  &trans  &trans    &trans    &trans    &trans
&trans  &trans  &trans  &trans  &trans  &trans      &trans  &trans  &mkp MB4  &mkp MB4  &mkp MB5  &mkp MB5
&trans  &trans  &trans  &trans  &trans  &trans      &trans  &trans                                &trans
            >;
        };
    };
};
